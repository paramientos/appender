{
    "name": "soysaltan/appender",
    "description": "This is my package Appender",
    "keywords": [
        "soysaltan",
        "laravel",
        "appender"
    ],
    "homepage": "https://github.com/soysaltan/appender",
    "license": "MIT",
    "authors": [
        {
            "name": "sosi",
            "email": "soysaltann@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Soysaltan\\Appender\\": "src",
            "Soysaltan\\Appender\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soysaltan\\Appender\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soysaltan\\Appender\\AppenderServiceProvider"
            ],
            "aliases": {
                "Appender": "Soysaltan\\Appender\\AppenderFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
